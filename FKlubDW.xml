<?xml version="1.0"?>
<Schema name="FKlubDW">

 <!-- Define your cube incl. its dimensions and measures here. See FoodMart.xml for an example and http://mondrian.pentaho.com/documentation/schema.php for documentation -->

  <Dimension name="Product">
    <Hierarchy hasAll="true" primaryKey="product_id">
      <Table name="product"/>
      <Level name="name" column="name" uniqueMembers="true">
        <Property name="Price" column="price" type="Money"/>
        <Property name="Is Active" column="is_active" type="Boolean"/>
        <Property name="Deactivation Date" column="deactivation_date" type="Date"/>
      </Level>
    </Hierarchy>
  </Dimension>

  <Dimension name="Member">
    <Hierarchy hasAll="true" primaryKey="member_id">
      <Table name="member"/>
      <Level name="" column="" uniqueMembers="true">
        <Property name="Balance" column="balance" type="Money"/>
        <Property name="Is Active" column="is_active" type="Boolean"/>
        <Property name="Year of Enrollment" column="year" type="Numerical"/>
      </Level>
    </Hierarchy>
  </Dimension>

  <Dimension name="Room">
    <Hierarchy hasAll="true" primaryKey="room_id">
      <Table name="room"/>
      <Level name="Name" column="name" uniqueMembers="true">
        <Property name="Description" column="description" type="String"/>
      </Level>
    </Hierarchy>
  </Dimension>

